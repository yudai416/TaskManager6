        </activity>
        <activity android:name="Option" android:noHistory="true" android:label="@string/app_name"></activity><activity android:label="@string/app_name" android:name="AllList"></activity>
        <activity android:name="IgnoreList" android:label="@string/app_name"></activity>
        <activity android:name="KillList" android:label="@string/app_name"></activity>

    </application>
</manifest>